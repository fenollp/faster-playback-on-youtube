{
  "manifest_version": 3,
  "name": "Faster Playback on YouTube",
  "version": "1.2.0",
  "description": "YouTube's < & > only go so far, let's have them go to infinity.",

  "icons": { "16": "icons/16.png", "48": "icons/48.png", "128": "icons/128.png" },

  "content_scripts": [{
    "all_frames": true,
    "js": [ "ke.js" ],
    "matches": [ "https://www.youtube.com/*" ],
    "run_at": "document_start"
  }]
}
